<!--
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  Oracle JDeveloper BPEL Designer 
  
  Created: Thu Apr 19 09:36:23 EDT 2018
  Author:  ryanwilliams
  Type: BPEL 2.0 Process
  Purpose: One Way BPEL Process
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
-->
<process name="BPELProcessHelloWorld"
               targetNamespace="http://xmlns.oracle.com/ApplicationHelloWorld/ProjectHelloWorld/BPELProcessHelloWorld"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/ApplicationHelloWorld/ProjectHelloWorld/BPELProcessHelloWorld"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
               xmlns:ui="http://xmlns.oracle.com/soa/designer"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/salesforce/ApplicationHelloWorld/ProjectHelloWorld/SalesforceReference"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/salesforce/ApplicationHelloWorld/ProjectHelloWorld/SalesforceReferenceHelloWorld"
         xmlns:ns3="urn:sobject.enterprise.soap.sforce.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns4="urn:enterprise.soap.sforce.com"
         xmlns:oraext="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.ExtFunc"
         xmlns:bpm="http://xmlns.oracle.com/bpmn20/extensions"
         xmlns:xp20="http://www.oracle.com/XSL/Transform/java/oracle.tip.pc.services.functions.Xpath20"
         xmlns:ess="http://xmlns.oracle.com/scheduler" xmlns:hwf="http://xmlns.oracle.com/bpel/workflow/xpath"
         xmlns:xref="http://www.oracle.com/XSL/Transform/java/oracle.tip.xref.xpath.XRefXPathFunctions"
         xmlns:dvm="http://www.oracle.com/XSL/Transform/java/oracle.tip.dvm.LookupValue"
         xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/"
         xmlns:xdk="http://schemas.oracle.com/bpel/extension/xpath/function/xdk"
         xmlns:ids="http://xmlns.oracle.com/bpel/services/IdentityService/xpath"
         xmlns:ldap="http://schemas.oracle.com/xpath/extension/ldap"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/db/ApplicationHelloWorld/ProjectHelloWorld/OracleTestConnection"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/db/top/OracleTestConnection"
         xmlns:ns7="http://oracle.com/sca/soapservice/ApplicationHelloWorld/ProjectHelloWorld/SOAPService"
         xmlns:ns8="http://xmlns.oracle.com/singleString">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
   <import namespace="http://xmlns.oracle.com/pcbpel/adapter/salesforce/ApplicationHelloWorld/ProjectHelloWorld/SalesforceReferenceHelloWorld"
           location="../WSDLs/SalesforceReferenceHelloWorld.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"
           ui:processWSDL="true"/>
   <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="SalesforceReferenceHelloWorld" partnerLinkType="ns2:create_plt" partnerRole="create_role"/>
      <partnerLink name="SOAPService" partnerLinkType="ns7:SOAPService" myRole="execute_ptt"/>
   </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="gbl_Input_Var" messageType="ns2:createRequestMessage"/>
    <variable name="gbl_Output_Var" messageType="ns2:createResponseMessage"/>
      <variable name="Receive1_execute_InputVariable" messageType="ns7:requestMessage"/>
   </variables>
   <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in BPELProcessHelloWorld.wsdl) -->
      <receive name="Receive1" partnerLink="SOAPService" portType="ns7:execute_ptt" operation="execute"
               variable="Receive1_execute_InputVariable" createInstance="yes"/>
      <forEach parallel="no" counterName="ForEach1Counter" name="ForEach1">
         <startCounterValue>0</startCounterValue>
         <finalCounterValue>5</finalCounterValue>
         <scope name="Scope1">
            <if name="If1" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
               <documentation>
                  <![CDATA[input = test]]>
               </documentation>
               <condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$Receive1_execute_InputVariable = 'test'</condition>
               <sequence>
                  <if name="If2">
                     <condition>$ForEach1Counter mod 2 = 0</condition>
                     <sequence>
                        <assign name="Assign1">
                           <copy>
                              <from>1</from>
                              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Number_of_Failures__c</to>
                           </copy>
                           <copy>
                              <from>1</from>
                              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Process_Description__c</to>
                           </copy>
                           <copy>
                              <from>$Receive1_execute_InputVariable.SOAPInput</from>
                              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:Name</to>
                           </copy>
                           <copy>
                              <from>1</from>
                              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Number_of_Successes__c</to>
                           </copy>
                           <copy>
                              <from>1</from>
                              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Total_Records_Processed__c</to>
                           </copy>
                        </assign>
                        <invoke name="Invoke1" partnerLink="SalesforceReferenceHelloWorld"
                                portType="ns2:SalesforceReferenceHelloWorldPortType" operation="create"
                                inputVariable="gbl_Input_Var" outputVariable="gbl_Output_Var" bpelx:invokeAsDetail="no"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension"/>
                     </sequence>
                     <else>
                        <sequence name="Sequence4">
                           <assign name="Assign3">
                              <copy>
                                 <from>3</from>
                                 <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Number_of_Failures__c</to>
                              </copy>
                              <copy>
                                 <from>$Receive1_execute_InputVariable.SOAPInput</from>
                                 <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:Name</to>
                              </copy>
                              <copy>
                                 <from>3</from>
                                 <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Number_of_Successes__c</to>
                              </copy>
                              <copy>
                                 <from>3</from>
                                 <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Process_Description__c</to>
                              </copy>
                              <copy>
                                 <from>3</from>
                                 <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Total_Records_Processed__c</to>
                              </copy>
                           </assign>
                           <invoke name="Invoke2"
                                   partnerLink="SalesforceReferenceHelloWorld"
                                   portType="ns2:SalesforceReferenceHelloWorldPortType" operation="create"
                                   inputVariable="gbl_Input_Var" outputVariable="gbl_Output_Var"
                                   bpelx:invokeAsDetail="no"/>
                        </sequence>
                     </else>
                  </if>
               </sequence>
               <else>
                  <sequence name="Sequence2">
                     <assign name="Assign2" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                        <copy>
                           <from>2</from>
                           <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Number_of_Failures__c</to>
                        </copy>
                        <copy>
                           <from>2</from>
                           <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Number_of_Successes__c</to>
                        </copy>
                        <copy>
                           <from>$Receive1_execute_InputVariable.SOAPInput</from>
                           <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:Name</to>
                        </copy>
                        <copy>
                           <from>2</from>
                           <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Total_Records_Processed__c</to>
                        </copy>
                        <copy>
                           <from>2</from>
                           <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$gbl_Input_Var.parameters/ns2:TSA_CSC_System_Log__c/ns3:TSA_CSC_Process_Description__c</to>
                        </copy>
                     </assign>
                     <invoke name="Invoke1" partnerLink="SalesforceReferenceHelloWorld"
                             portType="ns2:SalesforceReferenceHelloWorldPortType" operation="create"
                             inputVariable="gbl_Input_Var" outputVariable="gbl_Output_Var" bpelx:invokeAsDetail="no"
                             xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                             xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/>
                  </sequence>
               </else>
            </if>
         </scope>
      </forEach>
  </sequence>
</process>